{
  "version": 2,
  "builds": [
    {
      "src": "Website/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/Website/index.html" },
    { "src": "/admin", "dest": "/Website/admin.html" },
    { "src": "/admin-login", "dest": "/Website/admin-login.html" },
    { "src": "/booking", "dest": "/Website/booking.html" },
    { "src": "/rider-login", "dest": "/Website/rider-login.html" },
    { "src": "/rider-dashboard", "dest": "/Website/rider-dashboard.html" },
    { "src": "/rider-signup", "dest": "/Website/rider-signup.html" },
    { "src": "/([^/]+\\.html)", "dest": "/Website/$1" },
    { "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|ico|svg|webp|json|xml|woff|woff2|ttf|eot))", "dest": "/Website/$1" },
    { "src": "/.*", "dest": "/Website/index.html" }
  ]
}

{
  "version": 2,
  "builds": [
    {
      "src": "**/*.{html,js,css,png,jpg,jpeg,gif,ico,svg,webp}",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|webp|json|xml)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
